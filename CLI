#include <iostream>
#include <stdio.h>
#include <string>

using namespace std;

int main() {
    string command = "dir";
    char buff[128];
    string result = "";
    FILE* pipe = popen(command.c_str(), "r");
    
    while(!feof(pipe)){
        
        if(fgets(buff,128,pipe)!=NULL);
        result += buff;
        
    }
    cout<<result<<endl;
    pclose(pipe);
    
}
